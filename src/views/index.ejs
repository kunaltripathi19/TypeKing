    <div class="wrapper">
      <nav class="navbar navbar-expand-lg bg-dark ">
        <div class="container-fluid">

            <form method="post" action="/logout">
              <button class="btn btn-danger" type="submit">Logout</button>
            </form>
            <a class="navbar-brand d-flex ml-auto display-2 text-white fs-2 fw-bold "
                style="font-family: 'cursive'; text-shadow: 0.5px 0.4px #1a1a1a" href="#">TypeKing</a>

        </div>
    </nav>
      <div class="fs-3 fw-bold m-3 text-Capitalize btn btn-light "
          style="font-family: 'NHaasGroteskDSPro-65Md'; margin-top: 3px !important; margin-bottom: 10px !important;">
           Welcome <%- name %>, Lets Start Typing
      </div>
      <input type="text" class="input-field">
      <div class="content-box">
        <div class="typing-text">
          <p></p>
        </div>
        <div class="content">
          <ul class="result-details">
            <li class="time">
              <p>Time Left:</p>
              <span><b>60</b>s</span>
            </li>
            <li class="mistake">
              <p>Mistakes:</p>
              <span>0</span>
            </li>
            <li class="wpm">
              <p>WPM:</p>
              <span>0</span>
            </li>
            <li class="cpm">
              <p>CPM:</p>
              <span>0</span>
            </li>
          </ul> 
          
          <button>Try Again</button>
       
        </div>
      </div>
    </div>
  


    <script src="/js/paragraphs.js"></script>
    <script src="/js/script.js"></script>
  
